<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="Content-Language" content="en">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>Technical QL Thông tin</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no, shrink-to-fit=no" />
    <meta name="description" content="This is an example dashboard created using build-in elements and components.">
    <meta name="msapplication-tap-highlight" content="no">
    <link href="~/Content/main.css" rel="stylesheet" />
    <script src="~/Content/assets/plotly-latest.min.js"></script>
    <script src="~/Content/jquery-3.0.0.min.js"></script>
    <script src="~/Content/bootstrap.min.js"></script>
    <link href="~/Content/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <script src="https://cdn.jwplayer.com/libraries/HV0iiS3W.js"></script>
</head>
<body onselectstart="return false" oncontextmenu="return false">
    <div class="app-container app-theme-white body-tabs-shadow fixed-sidebar fixed-header">
        <div class="app-header header-shadow">
            <div class="app-header__logo">

                <a class="navbar-brand" href="https://anphatedu.com/">
                    <img src="https://anphatedu.com/wp-content/uploads/thegem-logos/logo_50f422672fa87c0a9aec8e4714728e05_1x.png" alt="" height="50">
                </a>

                <div class="header__pane ml-auto">
                    <div>
                        <button type="button" class="hamburger close-sidebar-btn hamburger--elastic" data-class="closed-sidebar">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
            </div>
            <div class="app-header__mobile-menu">
                <div>
                    <button type="button" class="hamburger hamburger--elastic mobile-toggle-nav">
                        <span class="hamburger-box">
                            <span class="hamburger-inner"></span>
                        </span>
                    </button>
                </div>
            </div>
            <div class="app-header__menu">
                <span>
                    <button type="button" class="btn-icon btn-icon-only btn btn-primary btn-sm mobile-toggle-header-nav">
                        <span class="btn-icon-wrapper">
                            <i class="fa fa-ellipsis-v fa-w-6"></i>
                        </span>
                    </button>
                </span>
            </div>
            <div class="app-header__content">
                <div class="header-slogan">
                    <div class="widget-content">
                        <div class="slogan">
                            Quản Lý Thông Tin
                        </div>
                    </div>
                </div>
                <div class="app-header-right">
                    <div class="header-btn-lg pr-0">
                        <div class="widget-content p-0">
                            <div class="widget-content-wrapper">
                                
                                
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="app-main">
            <div class="app-sidebar sidebar-shadow">
                <div class="app-header__logo">
                    <div class="logo-src"></div>
                    <div class="header__pane ml-auto">
                        <div>
                            <button type="button" class="hamburger close-sidebar-btn hamburger--elastic" data-class="closed-sidebar">
                                <span class="hamburger-box">
                                    <span class="hamburger-inner"></span>
                                </span>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="app-header__mobile-menu">
                    <div>
                        <button type="button" class="hamburger hamburger--elastic mobile-toggle-nav">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
                <div class="app-header__menu">
                    <span>
                        <button type="button" class="btn-icon btn-icon-only btn btn-primary btn-sm mobile-toggle-header-nav">
                            <span class="btn-icon-wrapper">
                                <i class="fa fa-ellipsis-v fa-w-6"></i>
                            </span>
                        </button>
                    </span>
                </div>
                <div class="scrollbar-sidebar">
                    <div class="app-sidebar__inner">
                        <ul class="vertical-nav-menu">
                            <li class="app-sidebar__heading">Tổng quan</li>

                            <li>
                                <a class="a mm-active" href="https://anphatedu.com/">
                                    <i class="metismenu-icon fa fa-home"></i>
                                    Trang chủ
                                </a>
                            </li>


                            <li class="app-sidebar__heading">Quản trị</li>
                            <li>
                                <a class="a" href="@Url.Action("Index","StatisticPage")">
                                    <i class="metismenu-icon fa fa-user"></i>
                                    Thông tin
                                </a>
                            </li>
                           
                            
                        </ul>
                    </div>
                </div>
            </div>

            @RenderBody()

        </div>
    </div>

    <script type="text/javascript" src="~/Content/assets/scripts/main.js"></script>


    <style>
        .a {
            text-decoration: none
        }
    </style>
</body>
</html>
